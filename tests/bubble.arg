len = 10;
arr[len] = {1,5,10,3,67,3,2,88,43,8};

i = 0;
temp = 0;
j = 0;


WHILE(i < len - 1) {
	j = 0;
	WHILE(j < len - i - 1) {
		IF(arr[j] > arr[j+1]) {
			temp = arr[j];
			arr[j] = arr[j+1];
			arr[j+1] = temp;
		}
		j = j + 1;
	}
	i = i + 1;
}

i = 0;
WHILE(i < len) {
	PRINT("%x\n", arr[j]);
	i = i + 1;
}
